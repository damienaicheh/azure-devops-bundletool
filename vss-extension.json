{
    "manifestVersion": 1,
    "id": "bundletool-tasks",
    "name": "Bundletool Helper",
    "version": "1.3.0",
    "publisher": "damienaicheh",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Easily use bundletool in your pipelines",
    "icons": {
        "default": "icon.png"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "mobile",
        "android"
    ],
    "content": {
        "details": {
            "path": "OVERVIEW.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "links": {
        "support": {
            "uri": "https://github.com/damienaicheh/azure-devops-bundletool/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/damienaicheh/azure-devops-bundletool"
    },
    "files": [
        {
            "path": "tasks/InstallBundletool"
        },
        {
            "path": "tasks/AabConvertToUniversalApk"
        },
        {
            "path": "tasks/Bundletool"
        }
    ],
    "contributions": [
        {
            "id": "install-bundletool-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/InstallBundletool"
            }
        },
        {
            "id": "aab-convert-to-universal-apk-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/AabConvertToUniversalApk"
            }
        },
        {
            "id": "bundletool-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/Bundletool"
            }
        }
    ]
}